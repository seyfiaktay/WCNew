<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")*@
    @Styles.Render("~/Content/themes/base/css")
    @Styles.Render("~/bundles/css")
    <!-- styles -->
    <link href="~/Content/Scaffold/assets/css/bootstrap.css" rel="stylesheet">
    <link href="~/Content/Scaffold/assets/css/bootstrap-responsive.css" rel="stylesheet">
    <link href="~/Content/Scaffold/assets/css/docs.css" rel="stylesheet">
    <link href="~/Content/Scaffold/assets/css/prettyPhoto.css" rel="stylesheet">
    <link href="~/Content/Scaffold/assets/js/google-code-prettify/prettify.css" rel="stylesheet">
    <link href="~/Content/Scaffold/assets/css/camera.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,300|Open+Sans:400,300,300italic,400italic" rel="stylesheet">
    <link href="~/Content/Scaffold/assets/css/style.css" rel="stylesheet">
    <link href="~/Content/Scaffold/assets/color/success.css" rel="stylesheet">
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/Scaffold/assets/js/jquery.bxslider.min.css" rel="stylesheet" />
    <!-- fav and touch icons -->
    <link rel="shortcut icon" href="~/Content/Scaffold/assets/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/Scaffold/assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/Scaffold/assets/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/Scaffold/assets/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/Content/Scaffold/assets/ico/apple-touch-icon-57-precomposed.png">
</head>
<body>
    <header>
        <!-- Navbar
        ================================================== -->
        <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container">
                    <!-- logo -->
                    <a class="brand logo" href="@Url.Action("Index", "Home")">
                        <img style="width:30px" src="~/Content/Scaffold/assets/img/icon_new.jpg" alt="" />
                    </a>
                    <!-- end logo -->
                    <!-- top menu -->
                    <div>
                        <nav>
                            <ul class="nav topnav">
                                <li class="dropdown success active">
                                    <a href="@Url.Action("Index", "Home")"><i class="icon-home icon-white"></i> Home</a>
                                    <ul class="dropdown-menu">
                                        <li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, null)</li>
                                    </ul>
                                </li>
                                <li class="dropdown primary">
                                    <a href="#"><i class="icon-star icon-white"></i> Features</a>
                                    <ul class="dropdown-menu">
                                        <li hidden="hidden" id="checklistmain" >
                                            <a href="/ChecklistMains/Index">
                                                Checklist Main
                                            </a>
                                        </li>
                                        <li hidden="hidden" id="checklist">
                                            <a href="/Checklists/Index">
                                                Checklist
                                            </a>
                                        </li>

                                        @*<li>@Html.ActionLink("API", "Index", "Help", new { area = "", hidden="hidden" }, null)</li>
                                            <li>@Html.ActionLink("ChecklistMain", "Index", "ChecklistMains", new { area = "",hidden="hidden" }, null)</li>
                                            <li>@Html.ActionLink("Checklist", "Index", "Checklists", new { area = "",hidden="hidden" }, null)</li>*@
                                    </ul>
                                </li>
                                <li class="dropdown primary">
                                    <a href="#"><i class="icon-star icon-white"></i> Account</a>
                                    <ul id="accountlist" class="dropdown-menu">
                                        <li hidden="hidden" id="email">
                                            <a id="aemail" href="/Account/ChangePassword">
                                                Email
                                            </a>
                                        </li>
                                        <li hidden="hidden" id="login">
                                            <a href="/Account/Login">
                                                Login
                                            </a>
                                        </li>
                                        <li hidden="hidden" id="logout">
                                            @Html.ActionLink("Logout", "Logout", "Account", new { area = "" }, null)
                                        </li>

                                        <li id="register">
                                            <a href="/Account/Register">
                                                Register
                                            </a>
                                        </li>
                                    </ul>
                                </li>

                                <li class="inverse">
                                    <a href="contact.html"><i class="icon-envelope icon-white"></i> Contact</a>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <!-- end menu -->
                </div>
            </div>
        </div>

    </header>

    @* javascripts *@
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/toastr")
    @RenderSection("scripts", required: false)

    <script src="~/Content/Scaffold/assets/js/jquery.bxslider.min.js"></script>
    <script src="~/Content/Scaffold/assets/js/jquery.elastislide.js"></script>
    <script src="~/Content/Scaffold/assets/js/jquery.prettyPhoto.js"></script>
    <script src="~/Content/Scaffold/assets/js/hover/jquery-hover-effect.js"></script>
    <script src="~/Content/Scaffold/assets/js/hover/setting.js"></script>
    <script src="~/Content/Scaffold/assets/js/custom.js"></script>
    <script src="~/Scripts/custom.js"></script>
    <script type="text/javascript">
        $(function () { // will trigger when the document is ready

            $(".datepicker").datepicker({
                //dateFormat: 'yy-mm-dd'
                dateFormat: 'dd.mm.yy'
            });
        })
    </script>

    <script type="text/javascript">
        $(function () { // will trigger when the document is ready
            var token = '@TempData["username"]'
            if (token == '') {
                //toastr.error("Unauthorized Request");
                $("#login").removeAttr("hidden");
                return;
            }
            else {
                $("#aemail").text(token);
                $("#email").removeAttr("hidden");
                $("#logout").removeAttr("hidden");
                $("#checklistmain").removeAttr("hidden");
                $("#checklist").removeAttr("hidden");
            }

            //var item = document.getElementById("email");
            //$("#email").text(token.userName);
        })

    </script>

    <div class="" style="padding : 80px">
        @RenderBody()
        <hr />
        <!-- Footer
        ================================================== -->
        <footer class="footer">
            <div class="container">
                <div class="row">
                    <div class="span4">
                        <div class="widget">
                            <h4>About us</h4>
                            <address>
                                <strong>Scaffold company, Inc.</strong><br>
                                445 Mypark Ave, Suite 800<br>
                                Jakarta, Indonesia 14210<br>
                                <abbr title="Phone">P:</abbr> (123) 456-7890
                            </address>
                            <address>
                                <strong>Contact us</strong><br>
                                <a href="mailto:#">hello@websitename.com</a>
                            </address>
                        </div>
                    </div>
                    <div class="span4">
                        <div class="widget">
                            <h4>Browse pages</h4>
                            <ul class="nav nav-list regular">
                                <li class="nav-header">More from us</li>
                                <li><a href="#">Work for us</a></li>
                                <li><a href="#">Creative process</a></li>
                                <li><a href="#">Case study</a></li>
                                <li class="nav-header">Quick links</li>
                                <li><a href="#">Scaffold awwards</a></li>
                                <li><a href="#">Meet the team</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="span4">
                        <div class="widget">
                            <h4>Get email updates</h4>
                            <form class="form-horizontal" action="#" method="post">
                                <fieldset>
                                    <p>
                                        Sign up for email updates and we'll plant a tree for you through Trees for the Future.
                                    </p>
                                    <div class="input-prepend input-append">
                                        <input class="span2" id="appendedPrependedInput" type="text" placeholder="Email">
                                        <button class="btn btn-inverse" type="submit">Subscribe!</button>
                                    </div>
                                </fieldset>
                            </form>
                            <ul class="social_small">
                                <li class="facebook first"><a href="#" title="Facebook">Facebook</a></li>
                                <li class="twitt"><a href="#" title="Twitter">Twitter</a></li>
                                <li class="googleplus"><a href="#" title="google plus">Google plus</a></li>
                                <li class="flickr"><a href="#" title="flickr">Flickr</a></li>
                                <li class="dribbble"><a href="#" title="Dribbble">Dribbble</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="verybottom">
                <div class="container">
                    <div class="row">
                        <div class="span6">
                            <p>&copy; Scaffold - All right reserved</p>
                        </div>
                        <div class="span6">
                            <div class="pull-right">
                                <div class="credits">
                                    <!--
                                      All the links in the footer should remain intact.
                                      You can delete the links only if you purchased the pro version.
                                      Licensing information: https://bootstrapmade.com/license/
                                      Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/buy/?theme=Scaffold
                                    -->
                                    Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>


</body>
</html>



