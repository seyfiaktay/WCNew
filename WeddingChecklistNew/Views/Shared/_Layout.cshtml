<script type="text/javascript">
    $(function () { // will trigger when the document is ready
        var x = document.cookie;
        var token = x.includes("token");
        if (token == false) {
            $("#checklistmain").attr("hidden", "hidden");
            $("#logout").attr("hidden", "hidden");
            $("#email").attr("hidden", "hidden");
            $("#login").removeAttr("hidden");
            $("#register").removeAttr("hidden");
        }
        else {
            $("#login").attr("hidden", "hidden");
            $("#register").attr("hidden", "hidden");
            $("#checklistmain").removeAttr("hidden");
            $("#logout").removeAttr("hidden");
            $("#email").removeAttr("hidden");
        }

        $(".datepicker").datepicker({
            autoclose: true,
            todayHighlight: true,
            format: 'dd/mm/yyyy'
        });

        $('.zoom')
            .wrap('<span style="display:inline-block"></span>')
            .css('display', 'block')
            .parent()
            .zoom();
    })


    function Search() {
        var searchstr = $("#searchstr").val();
        location.href = "/Checklists/Search?searchstr=" + searchstr;
    }
</script>
